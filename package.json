        "@tarojs/plugin-platform-jd": "3.6.24",
        "@tarojs/plugin-platform-qq": "3.6.24",
        "@tarojs/plugin-platform-swan": "3.6.24",
        "@tarojs/plugin-platform-tt": "3.6.24",
        "@tarojs/plugin-platform-weapp": "3.6.24",
        "@tarojs/runtime": "3.6.24",
